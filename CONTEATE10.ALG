algoritmo "ConteAte10"
var
   valor, contador: inteiro
inicio
   contador <- 0
   escreva ("Quer contar até quanto?")
   leia (valor)
   enquanto (contador <= (valor-1)) faca
      contador <- contador + 1
      escreval (contador)
   fimenquanto
   escreval ("Terminei de contar")
fimalgoritmo